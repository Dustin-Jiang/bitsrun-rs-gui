{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "bitsrun-rs-gui",
  "version": "0.1.0",
  "identifier": "top.tsukino.bitsrun-rs-gui",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "BIT Srun GUI",
        "width": 400,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/256x256.png",
      "icons/512x512.png",
      "icons/icon.ico",
      "icons/icon.icns"
    ]
  }
}
